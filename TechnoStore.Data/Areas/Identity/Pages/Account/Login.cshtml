<!--begin::Body-->
<body id="kt_body" style="background-image: url(assets/media/bg/bg-10.jpg) " class="quick-panel-right demo-panel-right offcanvas-right header-fixed subheader-enabled page-loading">
    <!--begin::Main-->
    <div class="d-flex flex-column flex-root">
        <!--begin::Login-->
        <div class="login login-2 login-signin-on d-flex flex-column flex-lg-row flex-column-fluid bg-white" id="kt_login">
            <!--begin::Aside-->
            <div class="login-aside order-2 order-lg-1 d-flex flex-column-fluid flex-lg-row-auto bgi-size-cover bgi-no-repeat p-7 p-lg-10">
                <!--begin: Aside Container-->
                <div class="d-flex flex-row-fluid flex-column justify-content-between">
                    <!--begin::Aside body-->
                    <div class="d-flex flex-column-fluid flex-column flex-center mt-5 mt-lg-0">
                        <a href="#" class="mb-15 text-center">
                            <img src="~/Images/fec70d38264541c982e351d4f657b6d9.png" class="max-h-75px" alt="" />
                        </a>
                        <!--begin::Signin-->
                        <div class="login-signin">
                            <div class="mb-20">
                                <h5 class="display-3 my-7">تسجيل الدخول</h5>
                                <div class="text-muted font-weight-bold">الرجاء إدخال البريد الالكتروني وكلمة السر للدخول </div>
                            </div>
                            <!--begin::Form-->
                            <form class="form" method="post" asp-page="Login">
                                <div class="form-group mb-5">
                                    <input asp-for="Input.Email" class="form-control h-auto form-control-solid py-4 px-8" type="text" placeholder="Email" autocomplete="off" />
                                </div>
                                <div class="form-group mb-5">
                                    <input asp-for="Input.Password" class="form-control h-auto form-control-solid py-4 px-8" type="password" placeholder="Password" />
                                </div>
                                <div asp-validation-summary="All" class="text-danger"></div>
                                <div class="form-group d-flex flex-wrap justify-content-between align-items-center">
                                    <div class="checkbox-inline">
                                        <label class="checkbox m-0 text-muted">
                                            <input asp-for="Input.RememberMe" type="checkbox" name="remember" />
                                            <span></span>تذكرني
                                        </label>
                                    </div>
                                </div>
                                <button id="kt_login_signin_submit" class="btn btn-primary font-weight-bold px-9 py-4 my-3 mx-4">دخول</button>
                            </form>
                            <!--end::Form-->

                        </div>
                        <!--end::Signin-->
                        <!--begin::Forgot-->
                        <div class="login-form login-forgot">
                            <div class="text-center mb-10 mb-lg-20">
                                <h3 class="">Forgotten Password ?</h3>
                                <p class="text-muted font-weight-bold">Enter your email to reset your password</p>
                            </div>
                            <!--begin::Form-->
                            <form class="form" novalidate="novalidate" id="kt_login_forgot_form">
                                <div class="form-group py-3 border-bottom mb-10">
                                    <input class="form-control h-auto border-0 px-0 placeholder-dark-75" type="email" placeholder="Email" name="email" autocomplete="off" />
                                </div>
                                <div class="form-group d-flex flex-wrap flex-center">
                                    <button id="kt_login_forgot_submit" class="btn btn-primary font-weight-bold px-9 py-4 my-3 mx-2">Submit</button>
                                    <button id="kt_login_forgot_cancel" class="btn btn-light-primary font-weight-bold px-9 py-4 my-3 mx-2">Cancel</button>
                                </div>
                            </form>
                            <!--end::Form-->
                        </div>
                        <!--end::Forgot-->
                    </div>
                    <!--end::Aside body-->
                    <!--begin: Aside footer for desktop-->
                    <div class="d-flex flex-column-auto justify-content-between mt-15">
                        <div class="text-dark-50 font-weight-bold order-2 order-sm-1 my-2">© @DateTime.Now.Year Dental Clinic</div>
                        <div class="d-flex order-1 order-sm-2 my-2">
                            <button type="button" class="btn btn-hover-light-warning" data-toggle="modal" data-target="#exampleModalLong"><i class="flaticon2-information"></i>تواصل مع المطور</button>
                        </div>
                    </div>
                    <!--end: Aside footer for desktop-->
                </div>
                <!--end: Aside Container-->
            </div>
            <!--begin::Aside-->
            <!--begin::Content-->
            <div class="order-1 order-lg-2 flex-column-auto flex-lg-row-fluid d-flex flex-column p-7" style="background-image: url(assets/media/bg/bg-4.jpg); ">
                <!--begin::Content body-->
