                                </tr>
                                <tr>
                                    <td width="80px">0593401701</td>
                                    <td width="80px">رقم الجوال</td>
                                </tr>
                                <tr>
                                    <td width="80px">2862789</td>
                                    <td width="80px">رقم الهاتف</td>
                                </tr>
                                <tr>
                                    <td width="80px">00970593401701</td>
                                    <td width="80px">رقم الواتساب</td>
                                </tr>
                                <tr>
                                    <td width="80px">Selena Dental Clinic</td>
                                    <td width="80px">صفحة الفيس بوك</td>
                                </tr>
                                <tr>
                                    <td width="80px">SelenaDental</td>
                                    <td width="80px">صفحة انستقرام</td>
                                </tr>
                            </table>
                        </p>
                    </div>
                </div>
                <!--end::Content body-->
            </div>
            <!--end::Content-->
        </div>
        <!--end::Login-->
    </div>
    <!--end::Main-->
